#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules/vitepress/bin/node_modules:/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules/vitepress/node_modules:/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules:/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules/vitepress/bin/node_modules:/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules/vitepress/node_modules:/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules:/home/kimo/kimo-docs/vite-press/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules/vitepress/bin/vitepress.js" "$@"
else
  exec node  "$basedir/../../../../../vitepress@1.6.4_@algolia+client-search@5.36.0_markdown-it-mathjax3@5.2.0_postcss@8.5.6_search-insights@2.17.3/node_modules/vitepress/bin/vitepress.js" "$@"
fi
